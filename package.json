{
  "name": "cuke-ui",
  "version": "1.9.0",
  "description": "A React.js UI components for Web",
  "repository": "https://github.com/cuke-ui/cuke-ui.git",
  "homepage": "https://cuke-ui.github.io/cuke-ui-landing/",
  "author": "Jinke.Li <jkli@thoughtWorks.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "lib",
    "es",
    "dist",
    "LICENSE"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "start": "yarn dev",
    "clean": "rimraf dist lib es",
    "dev": "start-storybook -p 8080 -c .storybook",
    "upgrade": "yarn upgrade",
    "stylelint:fix": "stylelint 'components/**/*.less' --syntax less --fix",
    "eslint:fix": "eslint ./components --fix",
    "lint": "prettier --write \"components/**/*.js\" && yarn eslint:fix && yarn stylelint:fix",
    "test": "cross-env BABEL_ENV=test jest --config jest.config.js",
    "test:update": "yarn test -u",
    "coverage": "yarn test --coverage",
    "ci:coverage": "yarn coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build:docs": "build-storybook -c .storybook -o .out",
    "pub:docs": "yarn build:docs && storybook-to-ghpages --existing-output-dir=.out",
    "prepublish": "yarn build",
    "build": "yarn run clean && yarn build:lib && yarn build:es && yarn build:umd && yarn build:css",
    "build:css": "cd scripts && gulp",
    "build:lib": "cross-env OUTPUT_MODULE=commonjs babel components -d lib --ignore **/__tests__",
    "build:es": "babel components -d es --ignore **/__tests__",
    "build:umd": "webpack --config ./scripts/build.umd.js",
    "deploy": "./scripts/deploy.sh",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "components/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "storybook-deployer": {
    "gitUsername": "cuke-ui",
    "gitEmail": "1359518268@qq.com",
    "commitMessage": "docs: deploy docs"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "sideEffects": [
    "dist/*",
    "lib/*",
    "es/*",
    "*.less"
  ],
  "commitizen": {
    "path": "cz-conventional-changelog"
  },
  "release": {
    "prepare": [
      {
        "path": "@semantic-release/changelog"
      },
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ]
    ]
  },
  "keywords": [
    "cuke",
    "cuke-ui",
    "react",
    "tool",
    "react-component",
    "component",
    "components",
    "ui",
    "web",
    "framework",
    "frontend"
  ],
  "tags": [
    "cuke",
    "cuke-ui",
    "react",
    "react-ui",
    "component",
    "components",
    "javascript",
    "es6",
    "css3"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@semantic-release/changelog": "^3.0.2",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-info": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addon-notes": "^4.1.4",
    "@storybook/addon-options": "^4.1.4",
    "@storybook/addon-storysource": "^4.1.4",
    "@storybook/addon-viewport": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "@storybook/storybook-deployer": "^2.8.1",
    "autoprefixer": "^6.7.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-import": "^1.6.2",
    "chai": "^4.1.2",
    "commitlint": "^7.2.1",
    "conventional-changelog-cli": "^2.0.5",
    "copy-webpack-plugin": "latest",
    "coveralls": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-filesize": "^0.0.6",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemap": "^1.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "highlight": "^0.2.4",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "less": "^2.7.2",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "normalize.css": "^8.0.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss": "^6.0.12",
    "postcss-cli": "^4.1.1",
    "postcss-loader": "2.1.3",
    "power-assert": "^1.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-loader": "^2.4.0",
    "react-lowlight": "^2.0.0",
    "react-markdown": "2.5.1",
    "rimraf": "^2.6.0",
    "sinon": "^5.0.7",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.5.8",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "moment": "^2.24.0",
    "prop-types": "^15.6.2",
    "react-icons": "^3.3.0",
    "react-jinke-music-player": "^4.2.0",
    "react-turntable": "^1.2.7",
    "scroll-into-view-if-needed": "^2.2.20"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
